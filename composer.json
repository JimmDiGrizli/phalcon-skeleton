{
    "name": "getsky/pherlin",
    "description": "Skeleton for develop Phalcon applications",
    "keywords": [ "framework", "phalcon", "pherlin", "skeleton" ],
    "license": "MIT",
    "authors": [
        {
            "name": "jimmdigrizli",
            "email": "getmansk-y@yandex.ru"
        }
    ],
    "require": {
        "ext-phalcon": ">=1.2.4,<2",

        "getsky/phalcon-bootstrap": "1.4.x",
        "getsky/phalcon-skeleton-provider": ">=1.0.2",
        "getsky/pherlin-demo-module": "1.0.x",
        "getsky/pretty-exceptions": "dev-master",
        "maximebf/debugbar": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "codeception/codeception": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.5.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetSky\\": "src/"
        }
    }
}
